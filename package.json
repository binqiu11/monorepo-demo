{
	"name": "monorepo-demo",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"init": "pnpm install",
		"dev": "pnpm --filter @unifid-portal/rmax build:dev && pnpm --parallel --filter @unifid-portal/rmax --filter @unifid-portal/shell dev",
		"build": "pnpm run build:prod",
		"build:dev": "pnpm --filter @unifid-portal/rmax build:dev && pnpm --filter @unifid-portal/shell build:dev",
		"build:stage": "pnpm --filter @unifid-portal/rmax build:stage && pnpm --filter @unifid-portal/shell build:stage",
		"build:prod": "pnpm --filter @unifid-portal/rmax build:prod && pnpm --filter @unifid-portal/shell build:prod",
		"test:shell": "pnpm --filter @unifid-portal/rmax build && pnpm --filter @unifid-portal/shell test"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.10.0"
}
